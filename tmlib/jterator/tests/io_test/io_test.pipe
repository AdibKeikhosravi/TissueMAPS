---
# JTERATOR PIPELINE DESCRIPTOR FILE
# =================================
# 
# The working directory is by default the directory where the .pipe file 
# is located. So you can provide relative paths in your pipeline description.


# Define your project:
project:
  lib: ./
  description:


# Define how jobs are created and processed:
images:
  layers:
  - name: 


# Define path to modules (executable files) and handles (YAML files)
# and the interpreter program (full path or command for /usr/bin/env):
pipeline:
- module: '{lib}/initial_py_module.py'
  handles: handles/initial_py_module.handles
  active: true

- module: '{lib}/m_module.m'
  handles: handles/m_module.handles
  active: true

- module: '{lib}/r_module.r'
  handles: handles/r_module.handles
  active: true

- module: '{lib}/jl_module.jl'
  handles: handles/jl_module.handles
  active: false

- module: '{lib}/py_module.py'
  handles: handles/py_module.handles
  active: true
